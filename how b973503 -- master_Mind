[32m+[m[32m#!/bin/python3[m
[32m+[m[32m# MasterMind[m
[32m+[m[32m# by ICTROCN[m
[32m+[m[32m# v1.01qw		qq12[m
[32m+[m[32m# 15-8-2024[m
[32m+[m[32m# Last mod by DevJan : added loop for replay[m
[32m+[m[32mprint("MasterMind")[m
[32m+[m
[32m+[m[32mimport random[m
[32m+[m
[32m+[m[32mdef generate_Code(length=4, digits=6):[m
[32m+[m[32m    return [str(random.randint(1, digits)) for _ in range(length)][m
[32m+[m
[32m+[m[32mdef get_Feedback(secret, guess):[m
[32m+[m[32m    black_Pegs = sum(s == g for s, g in zip(secret, guess))[m
[32m+[m[41m    [m
[32m+[m[32m    # Count whites by subtracting black and calculating min digit frequency match[m
[32m+[m[32m    secret_Counts = {}[m
[32m+[m[32m    guess_Counts = {}[m
[32m+[m
[32m+[m[32m    for s, g in zip(secret, guess):[m
[32m+[m[32m        if s != g:[m
[32m+[m[32m            secret_Counts[s] = secret_Counts.get(s, 0) + 1[m
[32m+[m[32m            guess_Counts[g] = guess_Counts.get(g, 0) + 1[m
[32m+[m
[32m+[m[32m    white_Pegs = sum(min(secret_Counts.get(d, 0), guess_Counts.get(d, 0)) for d in guess_Counts)[m
[32m+[m[41m    [m
[32m+[m[32m    return black_Pegs, white_Pegs[m
[32m+[m
[32m+[m[32mdef show_Secret(mystery):[m
[32m+[m[32m    print(mystery)[m
[32m+[m
[32m+[m[32mdef play_Mastermind():[m
[32m+[m[32m    print("Welcome to Mastermind!")[m
[32m+[m[32m    print("Guess the 4-digit code. Each digit is from 1 to 6. You have 10 attempts.")[m
[32m+[m[32m    secret_Code = generate_Code()[m
[32m+[m[32m    attempts = 10[m
[32m+[m
[32m+[m[32m    for attempt in range(1, attempts + 1):[m
[32m+[m[32m        guess = ""[m
[32m+[m[32m        valid_Guess = False[m
[32m+[m[32m        while not valid_Guess:[m
[32m+[m[32m            guess = input(f"Attempt {attempt}: ").strip()[m
[32m+[m[32m            valid_Guess = len(guess) == 4 and all(c in "123456" for c in guess)[m
[32m+[m[32m            if not valid_Guess:[m
[32m+[m[32m                print("Invalid input. Enter 4 digits, each from 1 to 6.")[m
[32m+[m[32m            show_Secret(secret_Code) if guess == "cheat" else False[m
[32m+[m
[32m+[m[32m        black, white = get_Feedback(secret_Code, guess)[m
[32m+[m[32m        print(f"Black pegs (correct position):" {black}, "White pegs (wrong position):" {white}        )[m
[32m+[m
[32m+[m[32m        if black == 4:[m
[32m+[m[32m            print(f"Congratulations! You guessed the code:" {''.join(secret_Code)})[m
[32m+[m[32m            return[m
[32m+[m
[32m+[m[32m    print(f"Sorry, you've used all attempts. The correct code was:" {''.join(secret_Code)})[m
[32m+[m
[32m+[m[32mif __name__ == "__main__":[m
[32m+[m[32m    again = 'Y'[m
[32m+[m[32m    while again == 'Y' :[m
[32m+[m[32m        play_Mastermind()[m
[32m+[m[32m        again  = input (f"Play again (Y/N) ?").upper()[m
[32m+[m
