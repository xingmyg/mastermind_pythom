[32m+[m[32m# MasterMiimport random[m
[32m+[m[32m# by ICTROCN[m
[32m+[m[32m# v1.01qw		qq12[m
[32m+[m[32m# 15-8-2024[m
[32m+[m[32m# Last mod by DevJan : added loop for replay[m
[32m+[m[32mprint("MasterMind")[m
[32m+[m
[32m+[m[32mimport random[m
[32m+[m
[32m+[m[32mVALID_COLORS = ['red', 'blue', 'yellow', 'green', 'purple', 'orange'][m
[32m+[m
[32m+[m[32mdef generate_code():[m
[32m+[m[32m    return [random.choice(colors) for _ in range(4)][m
[32m+[m
[32m+[m[32mdef get_feedback(secret, guess):[m
[32m+[m[32m    guess = [color.lower() for color in guess][m
[32m+[m
[32m+[m[32m    black_pegs = sum(s == g for s, g in zip(secret, guess))[m
[32m+[m[32m    secret_counts = {}[m
[32m+[m[32m    guess_counts = {}[m
[32m+[m
[32m+[m[32mdef is_admin():[m
[32m+[m[32m    password = input("Enter admin password: ")[m
[32m+[m[32m    return password == "letmein123"[m
[32m+[m
[32m+[m[32mdef show_secret(code):[m
[32m+[m[32m    print("Admin-only: The secret code is:", ' '.join(code))[m
[32m+[m
[32m+[m[32m    # Count whites by subtracting black and calculating min digit frequency match[m
[32m+[m[32m    secret_Code = generate_Code()[m
[32m+[m[32m    attempts = 10[m
[32m+[m
[32m+[m[32m    for s, g in zip(secret, guess):[m
[32m+[m[32m        if s != g:[m
[32m+[m[32m            secret_counts[s] = secret_counts.get(s, 0) + 1[m
[32m+[m[32m            guess_counts[g] = guess_counts.get(g, 0) + 1[m
[32m+[m
[32m+[m[32m    white_pegs = sum(min(secret_counts.get(color, 0), guess_counts.get(color, 0)) for color in guess_counts)[m
[32m+[m[32m    return black_pegs, white_pegs[m
[32m+[m
[32m+[m[32mdef play_Mastermind():[m
[32m+[m[32m    print("Welcome to Mastermind!")[m
[32m+[m[32m    print("Guess the 4 color code, Choose from:", ', '.join(VALID_COLORS))[m
[32m+[m[32m    print("You have 10 attempts.\n")[m
[32m+[m
[32m+[m[32m    secret_Code = generate_Code()[m
[32m+[m[32m    attempts = 10[m
[32m+[m
[32m+[m[32m    for attempt in range(1, attempts + 1):[m
[32m+[m[32m        valid_guess = False[m
[32m+[m[32m        while not valid_guess:[m
[32m+[m[32m            guess_input = input(f"Attempt {attempt}: ").strip().lower()[m
[32m+[m
[32m+[m[32m            if guess_input == "cheat":[m
[32m+[m[32m                if is_admin():[m
[32m+[m[32m                    show_secret(secret_code)[m
[32m+[m[32m                else:[m
[32m+[m[32m                    print("Access denied.")[m
[32m+[m[32m                continue[m
[32m+[m
[32m+[m[32m            guess_list = guess_input.split()[m
[32m+[m[32m            valid_guess = len(guess_list) == 4 and all(color in VALID_COLORS for color in guess_list)[m
[32m+[m
[32m+[m[32m        if not valid_Guess:[m
[32m+[m[32m           print("Invalid input. Please enter 4 valid colors separated by spaces.")[m
[32m+[m
[32m+[m[32m        black, white = get_feedback(secret_code, guess_list)[m
[32m+[m[32m        print(f"Black pegs (correct color & position): {black}")[m
[32m+[m[32m        print(f"White pegs (correct color, wrong position): {white}\n")[m
[32m+[m
[32m+[m[32m        if black == 4:[m
[32m+[m[32m    print(f"Congratulations! You guessed the code: {' '.join(secret_code)}")[m
[32m+[m[32m    return[m
[32m+[m
[32m+[m[32mprint(f"Game over! The correct code was: {' '.join(secret_code)}")[m
[32m+[m
[32m+[m
[32m+[m[32mif __name__ == "__main__":[m
[32m+[m[32m    again = 'Y'[m
[32m+[m[32m    while again == 'Y' :[m
[32m+[m[32m        play_Mastermind()[m
[32m+[m[32m        again  = input (f"Play again (Y/N) ?").upper()[m
[32m+[m
